<%- data.nextPage %>
<form>
    <!-- Save Button -->
    <input type="button" onclick="save()" value="SAVE"></input>

    <!-- Open editor Button -->
    <% if (data.type !== 'editor') { %>
        <input type="button" onclick="openEditor()" value="+"></input>
    <% } %>

    <!-- Go to next page Button -->
    <% if (typeof data.nextPage !== 'undefined' && typeof data.nextPage !== '' ) { %>
        <input type="button" onclick="next()" value="NEXT"></input>
    <% } %>

    <!-- Return to first page Button -->
    <input type="button" onclick="index()" value="INDEX"></input>
</form>