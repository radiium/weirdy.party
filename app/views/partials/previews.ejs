<div id="columns">

    <% for(var i=0; i < previews.length; i++) {%>

    <figure>
        <a href="/pages/<%- previews[i] %>">
        
        <!-- Thumbnail -->
        <img src="/prevs/<%- previews[i] %>.png" alt="Page <%- previews[i] %>">

        <!-- Caption -->
        <figcaption>
        <h3>/<%- previews[i] %></h3>
        </figcaption>

        </a>
    </figure>

    <% } %>

</div>
